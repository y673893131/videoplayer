QPushButton#btn_min{border-image:url(:/title/min_normal); border-radius:3px; outline: none;max-width: 20px; max-height: 20px; min-width: 20px;min-height: 20px;}QPushButton#btn_min::hover{border-image:url(:/title/min_hover);background: none;border-radius:10px; }

QPushButton#btn_max{border-image:url(:/title/max_normal); border-radius:10px; outline: none;max-width: 20px; max-height: 20px; min-width: 20px;min-height: 20px;}QPushButton#btn_max::hover{border-image:url(:/title/max_hover);background: none;border-radius:10px; }

QPushButton#btn_close{border-image:url(:/title/close_normal); border-radius:10px; outline: none;max-width: 20px; max-height: 20px; min-width: 20px;min-height: 20px;margin-right:5px;}QPushButton#btn_close::hover{border-image:url(:/title/close_hover);background: none;border-radius:10px; }

QPushButton#btn_topwindow{border-image:url(:/title/untopwindow); border-radius:10px; outline: none;max-width: 20px; max-height: 20px; min-width: 20px;min-height: 20px;margin-right:5px;}QPushButton#btn_topwindow::hover{border-image:url(:/title/untopwindow_hover);background: none;border-radius:10px; }

QPushButton#btn_topwindow::checked{border-image:url(:/title/topwindow);}QPushButton#btn_topwindow::checked::hover{border-image:url(:/title/topwindow_hover);}

