QSlider#slider_voice
{
        background-color: transparent;
}

QSlider#slider_voice::add-page:Vertical
{
        background-color:rgb(37, 168, 198);
        width:4px;
}
QSlider#slider_voice::sub-page:Vertical
{
        background-color: rgb(87, 97, 106);
        width:4px;
}
QSlider#slider_voice::groove:Vertical
{
        background: white;
        width: 6px;
        margin: 2px 0px;
}
QSlider#slider_voice::handle:Vertical
{
        height: 8px;
        width:8px;
        background: white;
        border-radius:4px;
        margin: -2px;
}

QSlider#slider_pro
{
   background-color: transparent;
}

QSlider#slider_pro::add-page:Horizontal
{
   background-color: rgba(240, 240, 240, 50);
   border-radius: 2px;
   height:4px;
}
QSlider#slider_pro::sub-page:Horizontal
{
   background-color: rgba(255, 45, 45, 50);
   border-radius: 2px;
   height:4px;
}
QSlider#slider_pro::groove:Horizontal
{
   background: rgba(240,240,240,128);
   border-radius: 1px;
   height: 2px;
   margin: 0px 1px;
}
QSlider#slider_pro::groove:Horizontal:hover
{
   background: rgba(240, 240, 240, 128);
   border-radius: 4px;
   height:4px;
}

QSlider#slider_pro::handle:Horizontal
{
    height: 6px;
    width: 6px;
    /*background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0.6 #D6843A,stop:0.778409 #FFFFFF);*/
    /*background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0 #FF5740, stop:0.5 #FF5740,radius:0.5,stop:0.501 rgba(255,87,64,50),stop:1 rgba(255,87,64,50));*/
    background: #FF4F4A;
    border-radius: 3px;
    margin: -3px -3px;
}

QSlider#slider_pro::handle:Horizontal:disabled
{
   height: 0px;
   width: 0px;
   border-radius:0px;
   background:transparent;
   margin: 0px;
}

